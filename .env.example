# GaussHub环境配置模板
# 复制此文件为.env.local并填入实际值

# AI服务配置
QWEN_API_KEY=your_qwen_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# 邮件服务配置
RESEND_API_KEY=your_resend_api_key_here

# GitHub集成
GITHUB_TOKEN=your_github_personal_access_token

# 企业微信配置
WECHAT_CORP_ID=your_corp_id
WECHAT_AGENT_ID=1000003
WECHAT_SECRET=your_wechat_secret
WECHAT_TOKEN=your_wechat_token
WECHAT_AES_KEY=your_wechat_aes_key
WECHAT_CALLBACK_URL=http://your-domain.com:3000/api/wechat/callback

# 文件存储配置
MINIO_ACCESS_KEY=your_minio_access_key
MINIO_SECRET_KEY=your_minio_secret_key
MINIO_ENDPOINT=minio:9000

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=gausshub
DB_USER=gausshub_user
DB_PASSWORD=your_secure_password

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# 应用配置
NODE_ENV=development
PORT=3000
JWT_SECRET=your_jwt_secret_key_here
API_BASE_URL=http://your-domain.com:3000

# 域名配置
DOMAIN=your-domain.com

# Docker配置
DOCKER_REGISTRY=your_docker_registry